<gts-info category="dtc-read">
<title>Body Electrical &gt; Air Conditioner &gt; Trouble Codes</title>
<gts-exec function-id="50001">
<gts-ecu ecu-id="121"/>
</gts-exec>
<app-communication>
<app-param name="process-mode">0</app-param>
