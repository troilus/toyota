<row>
<entry align="center" valign="middle">
<ptxt>DTC 编号</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>检测项目</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>DTC 检测条件</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>故障部位</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>存储器</ptxt>
</entry>
