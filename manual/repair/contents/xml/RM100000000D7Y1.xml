<tgroup cols="2">
<colspec colname="col1" colwidth="4.96in"/>
<colspec colname="col2" colwidth="2.12in"/>
<thead>
<row>
<entry align="center" valign="middle">
<ptxt>结果</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>转至</ptxt>
</entry>
</row>
</thead>
<tbody>
<row>
<entry align="left" valign="middle">
<ptxt>未输出 DTC。</ptxt>
</entry>
<entry align="center" valign="middle">
<judge-value label="1">
<ptxt>A</ptxt>
</judge-value>
</entry>
</row>
<row>
<entry align="left" valign="middle">
<ptxt>输出 DTC。</ptxt>
</entry>
<entry align="center" valign="middle">
<judge-value label="2">
<ptxt>B</ptxt>
</judge-value>
</entry>
</row>
</tbody>
</tgroup>
</table>
</spec>
</content6>
<results>
