<colspec colname="col4" colnum="4" colwidth="2.87in"/>
<tbody>
<row>
<entry align="center" valign="middle">
<ptxt>*a</ptxt>
</entry>
<entry>
<ptxt>未连接线束的零部件</ptxt>
<ptxt>（鼓风机 1 号风门伺服机构分总成）</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>*b</ptxt>
</entry>
<entry align="left" valign="middle">
<ptxt>再循环位置</ptxt>
</entry>
</row>
<row>
<entry align="center" valign="middle">
<ptxt>*c</ptxt>
</entry>
<entry>
<ptxt>新鲜空气位置</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>-</ptxt>
</entry>
<entry align="center" valign="middle">
<ptxt>-</ptxt>
</entry>
</row>
</tbody>
</tgroup>
</table>
</callout>
</figure>
</test1>
