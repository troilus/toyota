<?xml version="1.0" encoding="utf-8"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0"><xsl:output method="html" encoding="UTF-8" /><xsl:template match="/"><xsl:apply-templates select="connector_list" /></xsl:template><xsl:template match="connector_list"><HTML><HEAD><META http-equiv="Content-Type" content="text/html; charset=UTF-8" /><title>维修线束信息</title><!--<link rel="stylesheet" href="../repairWire.css" />--></HEAD><!-- body --><BODY bgcolor="#e9eeea"><xsl:apply-templates select="connector" /></BODY><!-- --></HTML></xsl:template><xsl:template match="connector"><TABLE border="1" cellpadding="5"><TR><TD bgcolor="#F0F0F0">线束类型</TD><TD bgcolor="#F0F0F0" align="center"><B><FONT size="+1" color="#000000"><xsl:if test="terminal/@alumi='Y'">铝</xsl:if><xsl:if test="terminal/@alumi='N'">铜</xsl:if></FONT></B></TD></TR><TR><TD bgcolor="#F0F0F0">端子类型</TD><TD bgcolor="#F0F0F0" align="center"><B><FONT size="+1" color="#000000"><xsl:value-of select="terminal/@type"/><xsl:if test="terminal/sealing/@ability='Y'"> （防水型）</xsl:if><xsl:if test="terminal/sealing/@ability='N'"> （非防水型）</xsl:if></FONT></B></TD></TR><TR><TD bgcolor="#EEEEEE">插头/插座</TD><TD bgcolor="#EEEEEE" align="center"><B><xsl:if test="@sex='F'">插座</xsl:if><xsl:if test="@sex='M'">插头</xsl:if></B></TD></TR><TR><TD colspan="2" bgcolor="#BBBBBB" valign="top" nowrap="nowrap"><xsl:choose><xsl:when test="terminal/@repair_id='-'">无此端子信息</xsl:when><xsl:when test="terminal/@repair_id=''">无此端子信息</xsl:when><xsl:otherwise><!--<TABLE border="0" cellpadding="1"><TR><TD valign="top" nowrap="nowrap"><xsl:apply-templates select="terminal" /><xsl:apply-templates select="terminal/housing_list" /></TD></TR></TABLE>--><div style="float:left; margin-right=5px; margin-bottom=5px"><xsl:apply-templates select="terminal" /></div><div style="float:left"><xsl:apply-templates select="terminal/housing_list" /></div></xsl:otherwise></xsl:choose></TD></TR></TABLE></xsl:template><xsl:template match="terminal"><xsl:variable name="sleeve-num" select="count(.//sleeve)" /><TABLE border="1" cellspacing="0" cellpadding="0" bordercolor="#FFFFFF"><TR><TH class="gray3_dark_left">维修线束</TH><xsl:if test="sealing/@ability='Y'"><TH class="gray3_dark_left">密封</TH></xsl:if><xsl:if test="$sleeve-num &gt; 0"><TH class="gray3_dark_left">套筒</TH></xsl:if></TR><TR><TD class="gray4_" valign="top"><xsl:attribute name="rowspan"><xsl:value-of select="$sleeve-num" /></xsl:attribute><IMG><xsl:attribute name="src">contents/connector/figrepair/<xsl:value-of select="@repair_id"/>.png</xsl:attribute></IMG><br/><xsl:apply-templates select="wire[1]" /></TD><xsl:if test="sealing/@ability='Y'"><TD class="gray4_" valign="top"><xsl:attribute name="rowspan"><xsl:value-of select="$sleeve-num" /></xsl:attribute><xsl:apply-templates select="sealing" /></TD></xsl:if><xsl:if test="$sleeve-num &gt; 0"><TD class="gray4_" valign="top"><xsl:apply-templates select="sleeve[1]" /></TD></xsl:if></TR><xsl:if test="$sleeve-num&gt;1"><TR><TD class="gray4_" valign="top"><xsl:apply-templates select="sleeve[2]" /></TD></TR></xsl:if><xsl:if test="$sleeve-num&gt;2"><TR><TD class="gray4_" valign="top"><xsl:apply-templates select="sleeve[3]" /></TD></TR></xsl:if><xsl:if test="$sleeve-num&gt;3"><TR><TD class="gray4_" valign="top"><xsl:apply-templates select="sleeve[4]" /></TD></TR></xsl:if><xsl:if test="$sleeve-num&gt;4"><TR><TD class="gray4_" valign="top"><xsl:apply-templates select="sleeve[5]" /></TD></TR></xsl:if></TABLE></xsl:template><xsl:template match="wire"><TABLE border="1" width="100%" cellspacing="0" cellpadding="3"><TR><TD rowspan="2" class="gray4_nopad">P/N:</TD><TD class="gray4_nopad">160mm</TD><TD class="gray4_nopad"><xsl:if test="count(parent::terminal/wire[@length='160mm']/repair_wire)='0'"><BR/></xsl:if><xsl:for-each select="parent::terminal/wire[@length='160mm']/repair_wire"><xsl:call-template name="repair_wire"/></xsl:for-each></TD></TR><TR><TD class="gray4_nopad">500 mm</TD><TD class="gray4_nopad"><xsl:if test="count(parent::terminal/wire[@length='500mm']/repair_wire)='0'"><BR/></xsl:if><xsl:for-each select="parent::terminal/wire[@length='500mm']/repair_wire"><xsl:call-template name="repair_wire"/></xsl:for-each></TD></TR></TABLE></xsl:template><xsl:template match="sealing"><xsl:apply-templates select="plug" /><br/><br/><xsl:apply-templates select="packing" /></xsl:template><xsl:template match="plug"><TABLE border="0" width="100%"><TR><TD class="gray4_nopad">孔塞</TD></TR><TR><TD class="gray4_nopad"><IMG><xsl:attribute name="src">contents/connector/figrepair/<xsl:value-of select="@fig" />.png</xsl:attribute></IMG></TD></TR><TR><TD class="gray4_nopad">P/N: <xsl:value-of select="@partNo"/></TD></TR></TABLE></xsl:template><xsl:template match="packing"><TABLE border="0" width="100%"><TR><TD class="gray4_nopad">端子密封件</TD></TR><TR><TD class="gray4_nopad"><IMG><xsl:attribute name="src">contents/connector/figrepair/<xsl:value-of select="@fig" />.png</xsl:attribute></IMG></TD></TR><TR><TD class="gray4_nopad">P/N: <xsl:value-of select="@partNo"/></TD></TR></TABLE></xsl:template><xsl:template name="repair_wire"><xsl:value-of select="@partNo"/><xsl:if test="@surface-treatment='Au'">（金）</xsl:if><br/></xsl:template><xsl:template match="sleeve"><TABLE border="0"><TR><TD><xsl:if test="@size='L'">大</xsl:if><xsl:if test="@size='M'">中</xsl:if><xsl:if test="@size='S'">小</xsl:if><xsl:if test="@color='Y'"> （黄色）</xsl:if><xsl:if test="@color='L'"> （蓝色）</xsl:if><xsl:if test="@color='R'"> （红色）</xsl:if><xsl:if test="@color='S1'"> （银色）</xsl:if><xsl:if test="@color='S2'"> （银色）</xsl:if></TD></TR><TR><TD><IMG><xsl:attribute name="src">contents/connector/figrepair/<xsl:value-of select="@fig"/>.png</xsl:attribute></IMG></TD></TR><TR><TD>P/N: <xsl:value-of select="@partNo"/></TD></TR></TABLE></xsl:template><xsl:template match="terminal/housing_list"><xsl:variable name="sleeve-num" select="count(.//sleeve)" /><TABLE border="1" cellspacing="0" cellpadding="0" bordercolor="#FFFFFF"><TR><TH class="gray3_dark_left">壳体横截面</TH></TR><xsl:for-each select="housing"><TR><TD class="gray4_"><TABLE border="0"><TR><TD><IMG><xsl:attribute name="src">contents/connector/figrepair/<xsl:value-of select="@fig"/>.png</xsl:attribute></IMG></TD></TR><TR><TD align="center"><xsl:if test="@lockType='D'">（双锁型）<BR/></xsl:if><xsl:if test="@lanceType='T'">[端子触片]</xsl:if><xsl:if test="@lanceType='H'">[壳体触片]</xsl:if></TD></TR></TABLE></TD></TR></xsl:for-each></TABLE></xsl:template></xsl:stylesheet>